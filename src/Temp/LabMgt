package lab;
import java.util.ArrayList;
import java.util.Scanner;

public class LabMgt {

	public static void lab(){
		int i = 0;
		int cont = 0;
		int[] labId = new int[50];
		String[] labName = new String[50];
		
		
		while(cont != 1){
			
		System.out.println("Enter lab name");
		Scanner sc = new Scanner(System.in);
	    labName[i] = sc.nextLine(); 
	    System.out.println("Enter lab ID");
	    labId[i] = sc.nextInt();
	    System.out.println("Terminate? Press 1? else press any key");
	    cont = sc.nextInt();
		i = i++;	
		
		}
		
		
		
	}
	
	
}
